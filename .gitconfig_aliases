[alias]
  s = status
  b = branch
  co = checkout
  ec = config --global -e # edit config
  ea = !$(git config core.editor) ~/.gitconfig.aliases
  lg = log --oneline
  edit = "!f() { $(git config core.editor) -- $@; }; f" # edit files
  main = "!remote=refs/remotes/origin/; head=$(git symbolic-ref ${remote}HEAD); heads=refs/heads/; git symbolic-ref refs/heads/_ ${head/$remote/$heads}; git branch --list _"